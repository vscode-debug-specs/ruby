{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug execute File",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bin/bin.rb"
    },
    {
      "name": "Debug test/unit",
      "type": "Ruby",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/test_unit/test_bubble_sort.rb",
      "args": ["--name=test_bubble_sort"]
    },
    {
      "name": "Debug remote",
      "type": "Ruby",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "remoteHost": "127.0.0.1",
      "remotePort": "1234",
      "remoteWorkspaceRoot": "/workspaces/ruby"
    }
  ]
}
