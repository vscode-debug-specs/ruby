{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug execute File",
			"type": "Ruby",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceRoot}/bin/bin.rb"
		},
		{
			"name": "Debug test/unit",
			"type": "Ruby",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceRoot}/test_unit/test_bubble_sort.rb",
			"args":[
				"--name=test_bubble_sort"
			]
		},
		{
			"name": "Debug remote",
			"type": "Ruby",
			"request": "attach",
			"cwd": "${workspaceRoot}",
			"remoteHost": "192.168.1.24",
			"remotePort": "1234",
			"remoteWorkspaceRoot": "/home/nnyn/vscode-debug-specs/ruby"
		},
	]
}